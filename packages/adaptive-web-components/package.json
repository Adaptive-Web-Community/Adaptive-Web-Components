{
    "name": "@adaptive-web/adaptive-web-components",
    "version": "0.1.0",
    "description": "Standards based, framework agnostic web components built using FAST.",
    "type": "module",
    "sideEffects": false,
    "license": "MIT",
    "author": {
        "name": "Adaptive Web Community",
        "url": "https://github.com/adaptive-web-community"
    },
    "bugs": {
        "url": "https://github.com/adaptive-web-community/adaptive-web-components/issues"
    },
    "homepage": "https://github.com/adaptive-web-community/adaptive-web-components#readme",
    "repository": {
        "type": "git",
        "url": "https://github.com/adaptive-web-community/adaptive-web-components.git",
        "directory": "packages/adaptive-web-components"
    },
    "main": "dist/esm/index.js",
    "module": "dist/esm/index.js",
    "types": "dist/dts/index.d.ts",
    "scripts": {
        "build:tsc": "tsc -p ./tsconfig.build.json",
        "build": "rimraf clean ./dist && npm run build:tsc && npm run cem:analyze",
        "build:storybook": "build-storybook",
        "clean": "rimraf dist storybook-static",
        "cem:analyze": "cem analyze --config ./cem.config.mjs",
        "lint": "eslint . --ext .ts",
        "lint:fix": "eslint . --ext .ts --fix",
        "start": "start-storybook -p 6006",
        "test": "npm run lint && npm run build:tsc"
    },
    "dependencies": {
        "@adaptive-web/adaptive-ui": "^0.0.1",
        "@fluentui/svg-icons": "^1.1.192",
        "@microsoft/fast-element": "2.0.0-beta.21",
        "@microsoft/fast-foundation": "3.0.0-alpha.25",
        "@microsoft/fast-web-utilities": "6.0.0",
        "tslib": "^2.5.0"
    },
    "peerDependencies": {
        "@microsoft/fast-foundation": "3.0.0-alpha.25"
    },
    "devDependencies": {
        "@babel/core": "^7.20.12",
        "@custom-elements-manifest/analyzer": "^0.6.8",
        "@custom-elements-manifest/to-markdown": "^0.1.0",
        "@storybook/addon-actions": "^6.5.16",
        "@storybook/addon-essentials": "^6.5.16",
        "@storybook/addon-links": "^6.5.16",
        "@storybook/builder-webpack5": "^6.5.16",
        "@storybook/csf": "0.0.2--canary.0899bb7.0",
        "@storybook/html": "^6.5.16",
        "@storybook/manager-webpack5": "^6.5.16",
        "@types/node": "^16.0.0",
        "@types/react": "^16.14.0",
        "babel-loader": "^8.3.0",
        "lit-html": "^2.5.0",
        "resolve-typescript-plugin": "^1.2.0",
        "rimraf": "^3.0.2",
        "source-map-loader": "^0.2.4",
        "svg-inline-loader": "^0.8.2",
        "ts-loader": "^7.0.2",
        "typescript": "^4.7.0"
    },
    "customElements": "dist/custom-elements.json",
    "exports": {
        "./custom-elements.json": "./dist/custom-elements.json",
        "./package.json": "./package.json",
        ".": {
            "types": "./dist/dts/index.d.ts",
            "default": "./dist/esm/index.js"
        },
        "./all-components": {
            "types": "./dist/dts/custom-elements.d.ts",
            "default": "./dist/esm/custom-elements.js"
        },
        "./accordion": {
            "types": "./dist/dts/components/accordion/accordion.definition.d.ts",
            "default": "./dist/esm/components/accordion/accordion.definition.js"
        },
        "./accordion-item": {
            "types": "./dist/dts/components/accordion-item/accordion-item.definition.d.ts",
            "default": "./dist/esm/components/accordion-item/accordion-item.definition.js"
        }
    }
}
